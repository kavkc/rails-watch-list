<%= cl_image_tag("y95zc2e06xnebtsoclg0", width:1550, height: 300, crop: :fill) %>
<div class="container">

  <h3>My Lists</h3>
    <div class="row">
      <% @lists.each do |list| %>
        <div class="co-12 col-sm-6 col-md-4 mb-3">
          <%= link_to list_path(list), class:"text-decoration-none" do %>
          <%= list.name %>
          <% end %>
        </div>
      <% end %>
    </div>
</div>
